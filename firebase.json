{
  "hosting": {
    "public": "frontend/out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/google/**",
        "destination": "https://mitra-api-yqrsbhg6ka-uc.a.run.app/google/:path",
        "type": 302
      },
      {
        "source": "/logout",
        "destination": "https://mitra-api-yqrsbhg6ka-uc.a.run.app/logout",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  }
}
